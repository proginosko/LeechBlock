												value="21600"/>
											<menuitem label="&options.halfDayMenuItem;"
												value="43200"/>
											<menuitem label="&options.dayMenuItem;"
												value="86400"/>
											<menuitem label="&options.twoDaysMenuItem;"
												value="172800"/>
											<menuitem label="&options.weekMenuItem;"
												value="604800"/>
										</menupopup>
