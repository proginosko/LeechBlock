									<checkbox id="lb-day36" label="&options.dayLabel3;"/>
									<checkbox id="lb-day46" label="&options.dayLabel4;"/>
									<checkbox id="lb-day56" label="&options.dayLabel5;"/>
									<checkbox id="lb-day66" label="&options.dayLabel6;"/>
